{
    "interpret": {
        "TEMPLATE": {
            "ibits": {
                "BIT1": {
                    "isfunc": false,
                    "iwith": ["BIT1", "OTHERREG:BIT2"],
                    "ipret": {
                        "0": "",
                        "1": ""
                    }
                },
                "BIT2": {
                    "isfunc": true,
                    "ipret": "",
                    "iwith": {
                        "VAR": ["BIT1", "OTHERREG:BIT2"]
                    },
                    "func": "3*VAR"
                }
            }
        },
        "INTS": {
            "ibitw": {
                "CELS": {
                    "isfunc": false,
                    "iwith": ["CELS"],
