                    "isfunc": false,
                    "iwith": ["FSARS"],
                    "ipret": {
                        "0": "ADC clock select set to 4Mhz",
                        "1": "ADC clock select set to 8Mhz"
                    }
                },
                "MVSEL1": {
                    "isfunc": false,
                    "iwith": ["MVSEL1", "MVSEL0"],
                    "ipret": {
                        "00": "ADC average times set to no average",
