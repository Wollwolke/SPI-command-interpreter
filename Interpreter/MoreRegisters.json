                    }
                }
            },
            "ibitr": {
                "ADC7": {
                    "isfunc": true,
                    "ipret": "Temperature value / ADC Reading (V) [Depending on ADC:XADS]",
                    "iwith": {
                        "ADC": ["ADC7", "ADC6", "ADC5", "ADC4", "ADC3", "ADC2", "ADC1", "ADC0"],
                        "XADS": ["ADC:XADS"]
                    },
                    "func": "if (XADS == 1, 0.3+1.2*ADC/256, ADC)"
                }
            }
